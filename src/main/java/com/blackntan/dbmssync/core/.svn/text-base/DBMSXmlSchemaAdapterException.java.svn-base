public class DBMSXmlSchemaAdapterException extends Throwable {
  public DBMSXmlSchemaAdapterException(Exception e) {
    super(e);
  }

  public DBMSXmlSchemaAdapterException() {
    super();
  }

  public DBMSXmlSchemaAdapterException(String s) {
    super(s);
  }

  public DBMSXmlSchemaAdapterException(String s, Throwable throwable) {
    super(s, throwable);
  }

  public DBMSXmlSchemaAdapterException(Throwable throwable) {
    super(throwable);
  }
}
